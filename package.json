{
  "name": "Sense",
  "version": "1.0.0",
  "description": "A JSON aware developer console to ElasticSearch.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . Sense --overwrite --asar --platform linux --ovewrite  --arch x64 --prune true --out dist/",
    "deb64": "electron-installer-debian --src dist/Sense-linux-x64 --dest dist/installers/ --arch amd64 --config config.json"
  },
  "repository": "https://github.com/icharlie/sense-app",
  "keywords": [
    "sense",
    "app"
  ],
  "author": "Charlie",
  "license": "CC0-1.0",
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^1.8.7",
    "electron-installer-debian": "^0.8.1",
    "electron-packager": "^12.1.0",
    "electron-reload": "^1.2.2"
  },
  "dependencies": {}
}
